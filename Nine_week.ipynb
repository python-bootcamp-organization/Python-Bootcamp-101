    "# finding an element by specific attribute key-values\n",
    "page = requests.get(\"https://github.com/Connor-SM\")\n",
    "soup = BeautifulSoup(page.content, \"html.parser\")\n",
    "username = soup.find( \"span\", attrs={ \"class\" : \"vcard-username\" })\n",
